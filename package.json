{
  "name": "castillo-next-app",
  "version": "0.2.0",
  "description": "Castillo is a next application for a vacation home rental business",
  "main": "index.js",
  "repository": "https://github.com/Davestring/castillo-next-app.git",
  "author": "David Martinez <davestringm@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "js:lint": "eslint --format stylish ./src",
    "js:lint:fix": "eslint --fix --format stylish ./src",
    "js:prettier": "prettier --write ./src/**/*.js",
    "lint": "yarn js:lint",
    "lint:fix": "yarn js:lint:fix",
    "dev": "next",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.3.4",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "framer-motion": "^3.10.0",
    "lodash": "^4.17.21",
    "next": "^10.0.8",
    "react": "^17.0.1",
    "react-div-100vh": "^0.5.6",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,md,html}": [
      "prettier --write"
    ]
  }
}
